datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Code {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  code        String
  name        String
  type        String
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
}
